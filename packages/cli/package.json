{
  "name": "@d4kmor/cli",
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Rocket stuff",
  "license": "MIT",
  "author": "Modern Web <hello@modern-web.dev> (https://modern-web.dev/)",
  "bin": {
    "rocket": "src/cli.js"
  },
  "scripts": {
    "build": "npm run rocket:build",
    "dev": "node ./eleventyOnly.js",
    "rocket:build": "node src/cli.js build -c demo",
    "rocket:start": "node src/cli.js start -c demo",
    "start": "npm run rocket:start",
    "test": "mocha test/**/*.test.js test/*.test.js",
    "test:watch": "onchange 'src/**/*.{js,cjs}' 'test/**/*.js' -- npm test",
    "xtest:watch": "mocha test/**/*.test.js --parallel --watch"
  },
  "files": [
    "*.cjs",
    "*.js",
    "*.mjs",
    "dist",
    "src"
  ],
  "keywords": [
    "rocket",
    "docs",
    "ssg",
    "demo",
    "11ty",
    "rollup"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@d4kmor/eleventy-plugin-mdjs-unified": "^0.3.0",
    "@d4kmor/eleventy-rocket-nav": "^0.2.1",
    "@open-wc/building-rollup": "^1.8.0",
    "@web/config-loader": "^0.1.2",
    "@web/dev-server": "^0.0.10",
    "@web/rollup-plugin-copy": "^0.1.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "deepmerge": "^4.2.2",
    "glob": "^7.1.6",
    "rollup": "^2.16.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-workbox": "^5.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.1.5",
    "@d4kmor/core": "^0.1.0",
    "@d4kmor/launch": "^0.4.0",
    "patch-package": "^6.2.2"
  },
  "exports": {
    "require": "./index.cjs"
  },
  "type": "module"
}
