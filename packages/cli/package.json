{
  "name": "@d4kmor/cli",
  "version": "0.6.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Rocket stuff",
  "license": "MIT",
  "author": "Modern Web <hello@modern-web.dev> (https://modern-web.dev/)",
  "bin": {
    "rocket": "src/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "npm run rocket:build",
    "dev": "node ./eleventyOnly.js",
    "rocket:build": "node src/cli.js build -c demo",
    "rocket:start": "node src/cli.js start -c demo",
    "start": "npm run rocket:start",
    "test": "mocha test/**/*.test.{js,cjs} test/*.test.{js,cjs}",
    "test:watch": "onchange 'src/**/*.{js,cjs}' 'test/**/*.{js,cjs}' -- npm test",
    "xtest:watch": "mocha test/**/*.test.js --parallel --watch"
  },
  "files": [
    "*.cjs",
    "*.js",
    "*.mjs",
    "dist",
    "src"
  ],
  "keywords": [
    "rocket",
    "docs",
    "ssg",
    "demo",
    "11ty",
    "rollup"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@d4kmor/core": "^0.1.2",
    "@d4kmor/eleventy-plugin-mdjs-unified": "^0.3.1",
    "@d4kmor/eleventy-rocket-nav": "^0.2.1",
    "@open-wc/building-rollup": "^1.9.2",
    "@web/config-loader": "^0.1.2",
    "@web/dev-server": "^0.0.11",
    "@web/rollup-plugin-copy": "^0.1.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "fs-extra": "^9.0.1",
    "rollup": "^2.16.0",
    "rollup-plugin-workbox": "^5.0.1"
  },
  "devDependencies": {
    "@d4kmor/launch": "^0.5.0",
    "@types/fs-extra": "^9.0.1"
  },
  "exports": {
    "require": "./index.cjs"
  }
}
